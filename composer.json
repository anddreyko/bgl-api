{
    "name": "anddreyko/bgl-api",
    "type": "project",
    "authors": [
        {
            "name": "anddreyko"
        }
    ],
    "autoload": {
        "files": [
            "helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "src/",
            "Actions\\": "actions/",
            "Commands\\": "commands/",
            "Fixtures\\": "fixtures/",
            "Migrations\\": "migrations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "soft-packages": true,
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "composer": "*",
        "doctrine/migrations": "^3.6",
        "doctrine/orm": "^2.14",
        "firebase/php-jwt": "^6.5",
        "guzzlehttp/guzzle": "^7.6",
        "kudashevs/accept-language": "^3.1",
        "laminas/laminas-config-aggregator": "^1.13",
        "monolog/monolog": "^3.3",
        "php-di/php-di": "^7.0",
        "ramsey/uuid": "^4.7",
        "sentry/sdk": "^3.3",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.11",
        "symfony/cache": "^6.3",
        "symfony/config": "^6.3",
        "symfony/mailer": "^6.2",
        "symfony/translation": "^6.3",
        "symfony/validator": "^6.3",
        "twig/twig": "^3.5",
        "vlucas/phpdotenv": "^3.3",
        "zircote/swagger-php": "^4.7"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "*",
        "codeception/module-phpbrowser": "*",
        "codeception/module-rest": "^3.3",
        "doctrine/data-fixtures": "^1.6",
        "overtrue/phplint": "^9.0",
        "phpstan/phpstan": "^1.10",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "*",
        "vimeo/psalm": "^5.8"
    },
    "scripts": {
        "lint": "phplint",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "psalm": "psalm",
        "phpstan": "phpstan",
        "test": "codecept run"
    }
}
